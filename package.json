{
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0"
  },
  "name": "faster-than-light",
  "version": "1.0.0",
  "description": "Faster Than Light smart contracts",
  "main": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.10",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-truffle": "^0.3.1",
    "esm": "^3.2.25",
    "eth-gas-reporter": "^0.2.22",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.2.0",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.3.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^6.0.0",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "truffle": "^5.3.6",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "solhint": "solhint 'contracts/**/*.sol'",
    "solhint:fix": "solhint --fix 'contracts/**/*.sol'",
    "compile": "hardhat compile",
    "migrate": "truffle migrate",
    "ganache": "ganache-cli",
    "test": "node --require esm node_modules/.bin/hardhat test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "coverage": "truffle run coverage --network coverage",
    "coveralls": "yarn coverage && cat coverage/lcov.info | coveralls",
    "prepare": "husky install",
    "gas-report": "env ENABLE_GAS_REPORT=1 npm run test"
  },
  "author": "David Kneel",
  "license": "ISC"
}
