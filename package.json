{
  "dependencies": {},
  "name": "faster-than-light",
  "version": "1.0.0",
  "description": "Faster Than Light smart contracts",
  "main": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.10",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "esm": "^3.2.25",
    "eth-gas-reporter": "^0.2.22",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.3.0",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.7.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.8",
    "hardhat-ethers": "^1.0.1",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^6.0.0",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "solhint": "solhint 'contracts/**/*.sol'",
    "solhint:fix": "solhint --fix 'contracts/**/*.sol'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "compile": "hardhat compile",
    "test": "node --require esm node_modules/.bin/hardhat test",
    "coverage": "node --require esm node_modules/.bin/hardhat coverage",
    "coveralls": "yarn coverage && cat coverage/lcov.info | coveralls",
    "prepare": "husky install",
    "gas-report": "env ENABLE_GAS_REPORT=1 npm run test",
    "deploy": "node --require esm node_modules/.bin/hardhat deploy --network",
    "deploy:legacy": "node --require esm node_modules/.bin/hardhat run scripts/deploy.js --network"
  },
  "author": "David Kneel",
  "license": "ISC"
}
